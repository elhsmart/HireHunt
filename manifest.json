{
  "manifest_version": 2,
  "name": "HireHunt",
  "version": "0.3.0",
  "description": "Small Kanban board for job seekers.",
  "minimum_chrome_version": "23",
  "icons": {

    "128": "assets/logo-128.png"
  },

  "app": {
    "background": {
      "scripts": ["js/upstart.js"]
    }
  },

  "permissions": [
    "storage",
    "unlimitedStorage",
    "<all_urls>",
    "http://*/*",
    "https://*/*"
  ]
}


