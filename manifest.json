{
  "manifest_version": 2,
  "name": "HireHunt",
  "version": "0.2",
  "minimum_chrome_version": "23",
  "icons": {

    "128": "assets/logo-128.png"
  },

  "app": {
    "background": {
      "scripts": ["js/upstart.js"]
    }
  },

  "permissions": [
    "storage",
    "unlimitedStorage",
    "<all_urls>",
    "http://*/*",
    "https://*/*"
  ]
}
